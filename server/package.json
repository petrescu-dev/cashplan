{
  "name": "cashplan-server",
  "version": "1.0.0",
  "description": "Backend server for Cashplan.io budget planning application",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "mocha --require tsx/cjs --extensions ts --recursive 'src/**/*.test.ts'",
    "test:watch": "mocha --require tsx/cjs --extensions ts --recursive 'src/**/*.test.ts' --watch",
    "test:coverage": "c8 npm test"
  },
  "keywords": ["budget", "financial-planning", "express", "typescript"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "cors": "^2.8.5",
    "better-sqlite3": "^11.7.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/cors": "^2.8.17",
    "@types/better-sqlite3": "^7.6.8",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/node": "^20.10.6",
    "@types/mocha": "^10.0.6",
    "@types/chai": "^4.3.11",
    "@types/sinon": "^17.0.2",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "mocha": "^10.2.0",
    "chai": "^4.3.10",
    "sinon": "^17.0.1",
    "c8": "^9.0.0"
  }
}

